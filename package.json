{
  "name": "preact-icu",
  "version": "0.0.0-0",
  "description": "tiny i18n provider with icu syntax for preact",
  "keywords": [
    "globalization",
    "i18n",
    "i18next",
    "icu",
    "internationalization",
    "localization",
    "preact",
    "translation"
  ],
  "homepage": "https://github.com/spurreiter/preact-icu#readme",
  "bugs": {
    "url": "https://github.com/spurreiter/preact-icu/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spurreiter/preact-icu.git"
  },
  "license": "MIT",
  "author": "spurreiter",
  "type": "module",
  "main": "./src/index.js",
  "types": "./types",
  "files": [
    "src",
    "types"
  ],
  "scripts": {
    "all": "npm-run-all lint test types",
    "build": "vite build",
    "dev": "vite",
    "coverage": "c8 jest",
    "lint": "eslint --fix --ext .js,.jsx .",
    "preview": "vite preview",
    "test": "jest",
    "types": "rimraf types; tsc --emitDeclarationOnly true --noEmitOnError && cp src/*.d.ts types"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".jsx"
    ],
    "preset": "jest-preset-preact",
    "setupFiles": [],
    "transform": {},
    "transformIgnorePatterns": []
  },
  "dependencies": {
    "intl-messageformat-tiny": "^1.0.2",
    "preact": "^10.11.3"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.4.0",
    "@testing-library/preact": "^3.2.2",
    "c8": "^7.12.0",
    "chai": "^4.3.7",
    "eslint": "^8.28.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-preset-preact": "^4.0.5",
    "npm-run-all": "^4.1.5",
    "preact-render-to-string": "^5.2.6",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.2",
    "typescript": "^4.9.3",
    "vite": "^3.2.4"
  }
}
